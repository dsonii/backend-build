{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    // "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-unused-vars":0,
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "eqeqeq":0,
    "linebreak-style":0,
    "import/newline-after-import":0,
    "curly":0,
    "indent":0,
    "qoutes":0,
    "semi":0,
    "prefer-arrow-callback":0,
    "func-names":0,
    "no-multiple-empty-lines":0,
    "no-tabs":0,
    "no-trailing-spaces":0,
    "eol-last":0,
    "comma-spacing":0,
    "padded-blocks":0,
    "global-requires":0,
    "comma-dangle":0,
    "new-cap":0,
    "prefer-template":0,
    "space-infix-ops":0,
    "spaced-comment":0,
    "max-len":0,
    "global-require":0,
    "no-undef":0,
    "camelcase":0,
    "key-spacing":0,
    "quotes":0,
    "consistent-return":0,
    "no-plusplus":0,
    "no-restricted-globals":0,
    "space-before-function-paren":0,
    "arrow-body-style":0,
    "keyword-spacing":0,
    "space-before-blocks":0,
    "brace-style":0,
    "no-shadow":0,
    "object-curly-spacing":0,
    "prefer-destructuring":0,
    "object-shorthand":0,
    "no-useless-escape":0,
    "no-unused-expressions":0,
    "object-curly-newline":0,
    "space-in-parens":0,
    "quote-props":0,
    "function-paren-newline":0,
    "no-return-await":0,
    "no-var":0,
    "prefer-const":0
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "extends": [
    "airbnb-base"
  ]
}
